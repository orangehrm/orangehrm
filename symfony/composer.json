{
    "name": "orangehrm/orangehrm",
    "description": "OrangeHRM open source",
    "require": {
        "php": "^7.4|^8.0",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-dom": "*",
        "ext-xml": "*",
        "symfony/http-foundation": "~4.4.19",
        "symfony/routing": "~4.4.19",
        "symfony/http-kernel": "~4.4.19",
        "symfony/event-dispatcher": "~4.4.19",
        "respect/validation": "1.1.11",
        "symfony/monolog-bundle": "^3.6",
        "bshaffer/oauth2-server-php": "^1.11",
        "doctrine/orm": "~2.8.2",
        "symfony/yaml": "^5.2",
        "twig/twig": "^3.0",
        "symfony/error-handler": "4.4.20",
        "symfony/dependency-injection": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Orangehrm\\Rest\\": "./plugins/orangehrmRESTPlugin/lib",
            "Orangehrm\\Rest\\Modules\\": "./plugins/orangehrmRESTPlugin/modules",
            "OrangeHRM\\ORM\\": "./lib/orm",
            "OrangeHRM\\Framework\\": "./lib/framework",
            "OrangeHRM\\Config\\": "./lib/config",
            "OrangeHRM\\Admin\\": "./plugins/orangehrmAdminPlugin",
            "OrangeHRM\\Core\\": "./plugins/orangehrmCorePlugin",
            "OrangeHRM\\Authentication\\": "./plugins/orangehrmAuthenticationPlugin",
            "OrangeHRM\\Entity\\": [
                "./plugins/orangehrmAdminPlugin/entity",
                "./plugins/orangehrmPerformancePlugin/entity",
                "./plugins/orangehrmPimPlugin/entity",
                "./plugins/orangehrmCorePlugin/entity"
            ],
            "": [
                "./plugins/orangehrmCoreOAuthPlugin/modules/oauth/actions",
                "./plugins/orangehrmAdminPlugin/lib/service",
                "./plugins/orangehrmAdminPlugin/lib/dao",
                "./apps/orangehrm/lib/model/exception"
            ]
        }
    }
}
